 <div class="beer">
    {{#each beersArr}}
        <img src="{{image_url}}" alt="">
        <p class="name">{{name}}</p>
        <p class="tagline">{{tagline}}</p>
        <p>{{description}}</p>
        
        {{> beerpartial}}
        <p>Food Pairing: {{food_pairing}}</p>
        <p>Brewer Tips: {{brewers_tips}}</p>
        
    {{/each}}
</div>


